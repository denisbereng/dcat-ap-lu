    skos:prefLabel "contact Point"@en .

dcat:distribution a owl:ObjectProperty ;
    skos:definition "An available Distribution for the Dataset."@en ;
    skos:prefLabel "distribution"@en .

dcat:downloadURL a rdf:Property ;

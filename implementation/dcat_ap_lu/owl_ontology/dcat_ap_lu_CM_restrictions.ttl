dcatap:applicablelegislation rdfs:domain [ a owl:Class ;
            owl:unionOf ( dcat:Dataset dcat:DatasetSeries dcat:Distribution ) ] ;
    rdfs:range eli:LegalResource .

dcterms:conformsTo rdfs:domain [ a owl:Class ;
            owl:unionOf ( dcat:Dataset dcat:Distribution ) ] ;
    rdfs:range dcterms:Standard .

dcterms:isReferencedBy rdfs:domain dcat:Dataset ;
    rdfs:range rdfs:Resource .

dcterms:language rdfs:domain [ a owl:Class ;
            owl:unionOf ( dcat:Dataset dcat:Distribution ) ] ;
    rdfs:range dcterms:LinguisticSystem .

dcterms:provenance rdfs:domain dcat:Dataset ;
    rdfs:range dcterms:ProvenanceStatement .

dcterms:relation rdfs:domain dcat:Dataset ;
    rdfs:range rdfs:Resource .

dcterms:source a owl:AsymmetricProperty ;
    rdfs:domain dcat:Dataset ;
    rdfs:range dcat:Dataset .

dcterms:spatial rdfs:domain [ a owl:Class ;
            owl:unionOf ( dcat:Dataset dcat:DatasetSeries ) ] ;
    rdfs:range dcterms:Location .

dcterms:temporal rdfs:domain dcat:Dataset ;
    rdfs:range dcterms:PeriodOfTime .

adms:identifier rdfs:domain dcat:Dataset ;
    rdfs:range adms:Identifier .

adms:sample rdfs:domain dcat:Dataset ;
    rdfs:range dcat:Distribution .

adms:versionNotes rdfs:domain dcat:Dataset ;
    rdfs:range rdfs:Literal .

dcat:accessService rdfs:domain dcat:Distribution ;
    rdfs:range dcat:DataService .

dcat:contactPoint rdfs:domain [ a owl:Class ;
            owl:unionOf ( dcat:Dataset dcat:DatasetSeries ) ] ;
    rdfs:range vcard:Kind .

dcat:distribution rdfs:domain dcat:Dataset ;
    rdfs:range dcat:Distribution .

dcat:downloadURL rdfs:domain dcat:Distribution ;
    rdfs:range rdfs:Resource .

dcat:hasVersion a owl:AsymmetricProperty ;
    rdfs:domain dcat:Dataset ;
    rdfs:range dcat:Dataset .

dcat:inSeries rdfs:domain dcat:Dataset ;
    rdfs:range dcat:DatasetSeries .

dcat:keyword rdfs:domain dcat:Dataset ;
    rdfs:range rdfs:Literal .

dcat:landingPage rdfs:domain dcat:Dataset ;
    rdfs:range foaf:Document .

dcat:qualifiedRelation rdfs:domain dcat:Dataset ;
    rdfs:range dcat:Relationship .

dcat:record rdfs:domain dcat:Catalog ;
    rdfs:range dcat:CatalogRecord .

dcat:servesDataset rdfs:domain dcat:Dataset ;
    rdfs:range dcat:DataService .

dcat:service rdfs:domain dcat:Catalog ;
    rdfs:range dcat:DataService .

dcat:theme rdfs:domain dcat:Dataset ;
    rdfs:range skos:Concept .

prov:qualifiedAttribution rdfs:domain dcat:Dataset ;
    rdfs:range prov:Attribution .

prov:qualifiedRelation rdfs:domain dcat:Relationship .

prov:wasGeneratedBy rdfs:domain dcat:Dataset ;
    rdfs:range prov:Activity .

foaf:page rdfs:domain [ a owl:Class ;
            owl:unionOf ( dcat:Dataset dcat:Distribution ) ] ;
    rdfs:range foaf:Document .

core-res:core-restriction a owl:Ontology ;
    rdfs:label "This artefact provides the restrictions and inference-related specifications for the eProcurement Ontology Core.         This artefact excludes the definitions of concepts.         The eDCAT-AP-LU describes objects and concepts, with definitions, attributes and relationships which are used for sharing information about Catalogues containing Datasets and Data Services descriptions in Europe.         The provision of these concepts provides the basis for a common understanding of the domain for all stakeholders ensuring the quality of data exchange and transparency."@en ;
    dcterms:created "2025-08-28"^^xsd:date ;
    dcterms:description "This artefact provides the restrictions and inference-related specifications for the eProcurement Ontology Core.         This artefact excludes the definitions of concepts.         The DCAT-AP-LU describes objects and concepts, with definitions, attributes and relationships which are used for sharing information about Catalogues containing Datasets and Data Services descriptions in Europe.         The provision of these concepts provides the basis for a common understanding of the domain for all stakeholders ensuring the quality of data exchange and transparency."@en ;

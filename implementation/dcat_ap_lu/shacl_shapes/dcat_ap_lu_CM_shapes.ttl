@prefix : <http://data.europa.eu/a4g/ontology#> .
@prefix adms: <http://www.w3.org/ns/adms#> .
@prefix at-voc: <http://publications.europa.eu/resource/authority/> .
@prefix bibo: <http://purl.org/ontology/bibo/> .
@prefix core-shape: <http://data.europa.eu/a4g/data-shape#> .
@prefix cv: <http://data.europa.eu/m8g/> .
@prefix dcat: <http://www.w3.org/ns/dcat#> .
@prefix dcatap: <http://data.europa.eu/r5r/> .
@prefix dcmitype: <http://purl.org/dc/dcmitype/> .
@prefix dcterms: <http://purl.org/dc/terms/> .
@prefix eli: <http://data.europa.eu/eli/ontology#> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix locn: <http://www.w3.org/ns/locn#> .
@prefix odrl: <http://www.w3.org/ns/odrl/2/> .
@prefix org: <http://www.w3.org/ns/org#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix prov: <http://www.w3.org/ns/prov#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix sh: <http://www.w3.org/ns/shacl#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix spdx: <http://spdx.org/rdf/terms#> .
@prefix time: <http://www.w3.org/2006/time#> .
@prefix vann: <http://purl.org/vocab/vann/> .
@prefix vcard: <http://www.w3.org/2006/vcard/ns#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

core-shape:adms-Identifier a sh:NodeShape ;
    rdfs:label "Identifier" ;
    rdfs:isDefinedBy core-shape:core-shape ;
    sh:property core-shape:adms-Identifier-skos-notation ;
    sh:targetClass adms:Identifier .

core-shape:dcat-Catalog a sh:NodeShape ;
    rdfs:label "Catalog" ;
    rdfs:isDefinedBy core-shape:core-shape ;
    sh:property core-shape:dcat-Catalog-dcat-record,
        core-shape:dcat-Catalog-dcat-service,
        core-shape:dcat-Catalog-dct-creator,
        core-shape:dcat-Catalog-dct-license,
        core-shape:dcat-Catalog-dct-publisher ;
    sh:targetClass dcat:Catalog .

core-shape:dcat-CatalogRecord a sh:NodeShape ;
    rdfs:label "Catalog Record" ;
    rdfs:isDefinedBy core-shape:core-shape ;
    sh:property core-shape:dcat-CatalogRecord-foaf-primaryTopic ;
    sh:targetClass dcat:CatalogRecord .

core-shape:dcat-DataService a sh:NodeShape ;
    rdfs:label "Data Service" ;
    rdfs:comment "A collection of operations that provides access to one or more datasets or data processing functions." ;
    rdfs:isDefinedBy core-shape:core-shape ;
    sh:targetClass dcat:DataService .

core-shape:dcat-Dataset a sh:NodeShape ;
    rdfs:label "Dataset" ;
    rdfs:isDefinedBy core-shape:core-shape ;
    sh:property core-shape:dcat-Dataset-adms-identifier,
        core-shape:dcat-Dataset-adms-sample,
        core-shape:dcat-Dataset-adms-versionNotes,
        core-shape:dcat-Dataset-dcat-contactPoint,
        core-shape:dcat-Dataset-dcat-distribution,
        core-shape:dcat-Dataset-dcat-inSeries,
        core-shape:dcat-Dataset-dcat-keyword,
        core-shape:dcat-Dataset-dcat-landingPage,
        core-shape:dcat-Dataset-dcat-servesDataset,
        core-shape:dcat-Dataset-dcat-spatialResolutionInMeters,
        core-shape:dcat-Dataset-dcat-temporalResolution,
        core-shape:dcat-Dataset-dcat-theme,
        core-shape:dcat-Dataset-dcatap-applicablelegislation,
        core-shape:dcat-Dataset-dct-accessRights,
        core-shape:dcat-Dataset-dct-accrualPeriodicity,
        core-shape:dcat-Dataset-dct-conformsTo,
        core-shape:dcat-Dataset-dct-creator,
        core-shape:dcat-Dataset-dct-description,
        core-shape:dcat-Dataset-dct-hasVersion,
        core-shape:dcat-Dataset-dct-identifier,
        core-shape:dcat-Dataset-dct-isReferencedBy,
        core-shape:dcat-Dataset-dct-isVersionOf,
        core-shape:dcat-Dataset-dct-issued,
        core-shape:dcat-Dataset-dct-language,
        core-shape:dcat-Dataset-dct-modified,
        core-shape:dcat-Dataset-dct-provenance,
        core-shape:dcat-Dataset-dct-publisher,
        core-shape:dcat-Dataset-dct-relation,
        core-shape:dcat-Dataset-dct-source,
        core-shape:dcat-Dataset-dct-spatial,
        core-shape:dcat-Dataset-dct-temporal,
        core-shape:dcat-Dataset-dct-title,
        core-shape:dcat-Dataset-dct-type,
        core-shape:dcat-Dataset-dpv-hasData,
        core-shape:dcat-Dataset-foaf-page,
        core-shape:dcat-Dataset-owl-versionInfo,
        core-shape:dcat-Dataset-prov-qualifiedAttribution,
        core-shape:dcat-Dataset-prov-qualifiedRelation,
        core-shape:dcat-Dataset-prov-wasGeneratedBy ;
    sh:targetClass dcat:Dataset .

core-shape:dcat-DatasetSeries a sh:NodeShape ;
    rdfs:label "Dataset Series" ;
    rdfs:comment "A collection of datasets that are published separately, but share some characteristics that group them. Usage Note It is recommended to avoid Dataset Series without a dataset in the collection. Therefore at least one Dataset should refer to a Dataset Series using the property in series (dcat:inSeries)." ;
    rdfs:isDefinedBy core-shape:core-shape ;
    sh:property core-shape:dcat-DatasetSeries-dcat-contactPoint,
        core-shape:dcat-DatasetSeries-dcatap-applicablelegislation,
        core-shape:dcat-DatasetSeries-dct-description,
        core-shape:dcat-DatasetSeries-dct-issued,
        core-shape:dcat-DatasetSeries-dct-modified,
        core-shape:dcat-DatasetSeries-dct-publisher,
        core-shape:dcat-DatasetSeries-dct-spatial,
        core-shape:dcat-DatasetSeries-dct-title ;
    sh:targetClass dcat:DatasetSeries .

core-shape:dcat-Distribution a sh:NodeShape ;
    rdfs:label "Distribution" ;
    rdfs:comment "A physical embodiment of the Dataset in a particular format." ;
    rdfs:isDefinedBy core-shape:core-shape ;
    sh:property core-shape:dcat-Distribution-adms-status,
        core-shape:dcat-Distribution-dcat-accessService,
        core-shape:dcat-Distribution-dcat-accessURL,
        core-shape:dcat-Distribution-dcat-byteSize,
        core-shape:dcat-Distribution-dcat-compressFormat,
        core-shape:dcat-Distribution-dcat-downloadURL,
        core-shape:dcat-Distribution-dcat-mediaType,
        core-shape:dcat-Distribution-dcat-packageFormat,
        core-shape:dcat-Distribution-dcat-spatialResolutionInMeters,
        core-shape:dcat-Distribution-dcat-temporalResolution,
        core-shape:dcat-Distribution-dcatap-applicablelegislation,
        core-shape:dcat-Distribution-dcatap-availability,
        core-shape:dcat-Distribution-dct-conformsTo,
        core-shape:dcat-Distribution-dct-description,
        core-shape:dcat-Distribution-dct-format,
        core-shape:dcat-Distribution-dct-issued,
        core-shape:dcat-Distribution-dct-language,
        core-shape:dcat-Distribution-dct-license,
        core-shape:dcat-Distribution-dct-modified,
        core-shape:dcat-Distribution-dct-rights,
        core-shape:dcat-Distribution-dct-title,
        core-shape:dcat-Distribution-foaf-page,
        core-shape:dcat-Distribution-odrl-hasPolicy,
        core-shape:dcat-Distribution-spdx-checksum ;
    sh:targetClass dcat:Distribution .

core-shape:dcat-Relationship a sh:NodeShape ;
    rdfs:label "Relationship" ;
    rdfs:isDefinedBy core-shape:core-shape ;
    sh:property core-shape:dcat-Relationship-dcat-qualifiedRelation ;
    sh:targetClass dcat:Relationship .

core-shape:dcat-Resource a sh:NodeShape ;
    rdfs:label "Resource" ;
    rdfs:comment "Resource published or curated by a single agent." ;
    rdfs:isDefinedBy core-shape:core-shape ;
    sh:targetClass dcat:Resource .

core-shape:dct-Frequency a sh:NodeShape ;
    rdfs:label "Frequency" ;
    rdfs:comment "A rate at which something recurs, e.g. the publication of a Dataset." ;
    rdfs:isDefinedBy core-shape:core-shape ;
    sh:targetClass dcterms:Frequency .

core-shape:dct-LicenseDocument a sh:NodeShape ;
    rdfs:label "License Document" ;
    rdfs:comment "A legal document giving official permission to do something with a resource." ;
    rdfs:isDefinedBy core-shape:core-shape ;
    sh:property core-shape:dct-LicenseDocument-dct-type ;
    sh:targetClass dcterms:LicenseDocument .

core-shape:dct-LinguisticSystem a sh:NodeShape ;
    rdfs:label "Linguistic System" ;
    rdfs:isDefinedBy core-shape:core-shape ;
    sh:targetClass dcterms:LinguisticSystem .

core-shape:dct-Location a sh:NodeShape ;
    rdfs:label "Location" ;
    rdfs:comment "A temporal period that the Dataset covers." ;
    rdfs:isDefinedBy core-shape:core-shape ;
    sh:property core-shape:dct-Location-dcat-bbox,
        core-shape:dct-Location-dcat-centroid,
        core-shape:dct-Location-locn-geometry ;
    sh:targetClass dcterms:Location .

core-shape:dct-MediaType a sh:NodeShape ;
    rdfs:label "Media Type" ;
    rdfs:comment "A file format or physical medium." ;
    rdfs:isDefinedBy core-shape:core-shape ;
    sh:targetClass dcterms:MediaType .

core-shape:dct-MediaTypeOrExtent a sh:NodeShape ;
    rdfs:label "Media Type Or Extent" ;
    rdfs:comment "A media type or dataset distribution." ;
    rdfs:isDefinedBy core-shape:core-shape ;
    sh:targetClass dcterms:MediaTypeOrExtent .

core-shape:dct-PeriodOfTime a sh:NodeShape ;
    rdfs:label "Period Of Time" ;
    rdfs:isDefinedBy core-shape:core-shape ;
    sh:property core-shape:dct-PeriodOfTime-dcat-endDate,
        core-shape:dct-PeriodOfTime-dcat-startDate,
        core-shape:dct-PeriodOfTime-time-hasBeginning,
        core-shape:dct-PeriodOfTime-time-hasEnd ;
    sh:targetClass dcterms:PeriodOfTime .

core-shape:dct-ProvenanceStatement a sh:NodeShape ;
    rdfs:label "Provenance Statement" ;
    rdfs:comment "Any changes in ownership and custody of a resource since its creation that are significant for its authenticity, integrity, and interpretation." ;
    rdfs:isDefinedBy core-shape:core-shape ;
    sh:targetClass dcterms:ProvenanceStatement .

core-shape:dct-RightsStatement a sh:NodeShape ;
    rdfs:label "Rights Statement" ;
    rdfs:comment "A statement about the intellectual property rights (IPR) held in or over a resource, a legal document giving official permission to do something with a resource, or a statement about access rights." ;
    rdfs:isDefinedBy core-shape:core-shape ;
    sh:targetClass dcterms:RightsStatement .

core-shape:dct-Standard a sh:NodeShape ;
    rdfs:label "Standard" ;
    rdfs:isDefinedBy core-shape:core-shape ;
    sh:targetClass dcterms:Standard .

core-shape:dpv-Data a sh:NodeShape ;
    rdfs:label "Data" ;
    rdfs:comment "A broad concept representing 'data' or 'information'" ;
    rdfs:isDefinedBy core-shape:core-shape ;
    sh:targetClass <https://w3id.org/dpv#Data> .

core-shape:eli-LegalResource a sh:NodeShape ;
    rdfs:label "Legal Resource" ;
    rdfs:comment "This class represents the legislation, policy or policies that lie behind the Rules that govern the service. Usage Note The definition and properties of the Legal Resource class are aligned with the ontology included in \"Council conclusions inviting the introduction of the European Legislation Identifier (ELI)\". For describing the attributes of a Legal Resource (labels, preferred labels, alternative labels, definition, etc.) we refer to the ELI ontology. In this data specification the use is restricted to instances of this class that follow the ELI URI guidelines" ;
    rdfs:isDefinedBy core-shape:core-shape ;
    sh:targetClass eli:LegalResource .

core-shape:foaf-Agent a sh:NodeShape ;
    rdfs:label "Agent" ;
    rdfs:isDefinedBy core-shape:core-shape ;
    sh:property core-shape:foaf-Agent-dct-type,
        core-shape:foaf-Agent-foaf-name ;
    sh:targetClass foaf:Agent .

core-shape:foaf-Document a sh:NodeShape ;
    rdfs:label "Document" ;
    rdfs:isDefinedBy core-shape:core-shape ;
    sh:targetClass foaf:Document .

core-shape:locn-Geometry a sh:NodeShape ;
    rdfs:label "Geometry" ;
    rdfs:comment "The Geometry class provides the means to identify a Location as a point, line, polygon, etc. expressed using coordinates in some coordinate reference System." ;
    rdfs:isDefinedBy core-shape:core-shape ;
    sh:targetClass locn:Geometry .

core-shape:odrl-Policy a sh:NodeShape ;
    rdfs:label "Policy" ;
    rdfs:comment "A non-empty group of Permissions and/or Prohibitions." ;
    rdfs:isDefinedBy core-shape:core-shape ;
    sh:targetClass odrl:Policy .

core-shape:prov-Activity a sh:NodeShape ;
    rdfs:label "Activity" ;
    rdfs:comment "An activity is something that occurs over a period of time and acts upon or with entities; it may include consuming, processing, transforming, modifying, relocating, using, or generating entities." ;
    rdfs:isDefinedBy core-shape:core-shape ;
    sh:targetClass prov:Activity .

core-shape:prov-Attribution a sh:NodeShape ;
    rdfs:label "Attribution" ;
    rdfs:comment "Attribution is the ascribing of an entity to an agent." ;
    rdfs:isDefinedBy core-shape:core-shape ;
    sh:targetClass prov:Attribution .

core-shape:skos-Concept a sh:NodeShape ;
    rdfs:label "Concept" ;
    rdfs:isDefinedBy core-shape:core-shape ;
    sh:property core-shape:skos-Concept-skos-prefLabel ;
    sh:targetClass skos:Concept .

core-shape:spdx-Checksum a sh:NodeShape ;
    rdfs:label "Checksum" ;
    rdfs:comment "A Checksum is value that allows the contents of a file to be authenticated. Even small changes to the content of the file will change its checksum. This class allows the results of a variety of checksum and cryptographic message digest algorithms to be represented." ;
    rdfs:isDefinedBy core-shape:core-shape ;
    sh:targetClass spdx:Checksum .

core-shape:vcard-Kind a sh:NodeShape ;
    rdfs:label "Kind" ;
    rdfs:comment "A description following the vCard specification, e.g. to provide telephone number and e-mail address for a contact point." ;
    rdfs:isDefinedBy core-shape:core-shape ;
    sh:targetClass vcard:Kind .

core-shape:adms-Identifier-skos-notation a sh:PropertyShape ;
    rdfs:isDefinedBy core-shape:core-shape ;
    sh:datatype rdfs:Literal ;
    sh:description "The literal identifying an entity, like a person or an object." ;
    sh:maxCount 1 ;
    sh:name "notation" ;
    sh:path skos:notation .

core-shape:dcat-Catalog-dcat-record a sh:PropertyShape ;
    sh:class dcat:CatalogRecord ;
    sh:name "record" ;
    sh:path dcat:record ;
    sh:sparql [ sh:select "SELECT ?this ?that WHERE { ?this <http://www.w3.org/ns/dcat#record> ?that . ?that <http://www.w3.org/ns/dcat#record> ?this .}" ] .

core-shape:dcat-Catalog-dcat-service a sh:PropertyShape ;
    sh:class dcat:DataService ;
    sh:name "service" ;
    sh:path dcat:service ;
    sh:sparql [ sh:select "SELECT ?this ?that WHERE { ?this <http://www.w3.org/ns/dcat#service> ?that . ?that <http://www.w3.org/ns/dcat#service> ?this .}" ] .

core-shape:dcat-Catalog-dct-creator a sh:PropertyShape ;
    sh:class foaf:Agent ;
    sh:maxCount 1 ;
    sh:name "creator" ;
    sh:path dcterms:creator ;
    sh:sparql [ sh:select "SELECT ?this ?that WHERE { ?this <http://purl.org/dc/terms/creator> ?that . ?that <http://purl.org/dc/terms/creator> ?this .}" ] .

core-shape:dcat-Catalog-dct-license a sh:PropertyShape ;
    sh:class dcterms:LicenseDocument ;
    sh:maxCount 1 ;
    sh:name "license" ;
    sh:path dcterms:license ;
    sh:sparql [ sh:select "SELECT ?this ?that WHERE { ?this <http://purl.org/dc/terms/license> ?that . ?that <http://purl.org/dc/terms/license> ?this .}" ] .

core-shape:dcat-Catalog-dct-publisher a sh:PropertyShape ;
    sh:class foaf:Agent ;
    sh:maxCount 1 ;
    sh:name "publisher" ;
    sh:path dcterms:publisher ;
    sh:sparql [ sh:select "SELECT ?this ?that WHERE { ?this <http://purl.org/dc/terms/publisher> ?that . ?that <http://purl.org/dc/terms/publisher> ?this .}" ] .

core-shape:dcat-CatalogRecord-foaf-primaryTopic a sh:PropertyShape ;
    sh:class dcat:Resource ;
    sh:maxCount 1 ;
    sh:minCount 1 ;
    sh:name "primary Topic" ;
    sh:path foaf:primaryTopic ;
    sh:sparql [ sh:select "SELECT ?this ?that WHERE { ?this <http://xmlns.com/foaf/0.1/primaryTopic> ?that . ?that <http://xmlns.com/foaf/0.1/primaryTopic> ?this .}" ] .

core-shape:dcat-Dataset-adms-identifier a sh:PropertyShape ;
    sh:class adms:Identifier ;
    sh:description "A secondary identifier of the Dataset." ;
    sh:name "identifier" ;
    sh:path adms:identifier ;
    sh:sparql [ sh:select "SELECT ?this ?that WHERE { ?this <http://www.w3.org/ns/adms#identifier> ?that . ?that <http://www.w3.org/ns/adms#identifier> ?this .}" ] .

core-shape:dcat-Dataset-adms-sample a sh:PropertyShape ;
    sh:class dcat:Distribution ;
    sh:description "A sample distribution of the dataset. Usage: DCAT-AP-LU provides additional usage guidance for the use of this property in providing data dictionaries as part of dataset descriptions." ;
    sh:name "sample" ;
    sh:path adms:sample ;
    sh:sparql [ sh:select "SELECT ?this ?that WHERE { ?this <http://www.w3.org/ns/adms#sample> ?that . ?that <http://www.w3.org/ns/adms#sample> ?this .}" ] .

core-shape:dcat-Dataset-adms-versionNotes a sh:PropertyShape ;
    rdfs:isDefinedBy core-shape:core-shape ;
    sh:datatype rdfs:Literal ;
    sh:description "A description of the differences between this version and a previous version of the Dataset." ;
    sh:name "version Notes" ;
    sh:path adms:versionNotes .

core-shape:dcat-Dataset-dcat-contactPoint a sh:PropertyShape ;
    sh:class vcard:Kind ;
    sh:description "Contact information that can be used for sending comments about the Dataset." ;
    sh:name "contact Point" ;
    sh:path dcat:contactPoint ;
    sh:sparql [ sh:select "SELECT ?this ?that WHERE { ?this <http://www.w3.org/ns/dcat#contactPoint> ?that . ?that <http://www.w3.org/ns/dcat#contactPoint> ?this .}" ] .

core-shape:dcat-Dataset-dcat-distribution a sh:PropertyShape ;
    sh:class dcat:Distribution ;
    sh:description "An available Distribution for the Dataset." ;
    sh:name "distribution" ;
    sh:path dcat:distribution ;
    sh:sparql [ sh:select "SELECT ?this ?that WHERE { ?this <http://www.w3.org/ns/dcat#distribution> ?that . ?that <http://www.w3.org/ns/dcat#distribution> ?this .}" ] .

core-shape:dcat-Dataset-dcat-inSeries a sh:PropertyShape ;
    sh:class dcat:DatasetSeries ;
    sh:description "A dataset series of which the dataset is part. Usage Note: DCAT-AP-LU recommends the use of DatasetSeries, with additional guidance provided" ;
    sh:name "in Series" ;
    sh:path dcat:inSeries ;
    sh:sparql [ sh:select "SELECT ?this ?that WHERE { ?this <http://www.w3.org/ns/dcat#inSeries> ?that . ?that <http://www.w3.org/ns/dcat#inSeries> ?this .}" ] .

core-shape:dcat-Dataset-dcat-keyword a sh:PropertyShape ;
    rdfs:isDefinedBy core-shape:core-shape ;
    sh:datatype rdfs:Literal ;
    sh:description "A keyword or tag describing the Dataset." ;
    sh:name "keyword" ;
    sh:path dcat:keyword .

core-shape:dcat-Dataset-dcat-landingPage a sh:PropertyShape ;
    sh:class foaf:Document ;
    sh:description "A web page that provides access to the Dataset, its Distributions and/or additional information." ;
    sh:name "landing Page" ;
    sh:path dcat:landingPage ;
    sh:sparql [ sh:select "SELECT ?this ?that WHERE { ?this <http://www.w3.org/ns/dcat#landingPage> ?that . ?that <http://www.w3.org/ns/dcat#landingPage> ?this .}" ] .

core-shape:dcat-Dataset-dcat-servesDataset a sh:PropertyShape ;
    sh:class dcat:DataService ;
    sh:name "serves Dataset" ;
    sh:path dcat:servesDataset ;
    sh:sparql [ sh:select "SELECT ?this ?that WHERE { ?this <http://www.w3.org/ns/dcat#servesDataset> ?that . ?that <http://www.w3.org/ns/dcat#servesDataset> ?this .}" ] .

core-shape:dcat-Dataset-dcat-spatialResolutionInMeters a sh:PropertyShape ;
    rdfs:isDefinedBy core-shape:core-shape ;
    sh:datatype xsd:decimal ;
    sh:description "The minimum spatial separation resolvable in a dataset, measured in meters." ;
    sh:maxCount 1 ;
    sh:name "spatial Resolution In Meters" ;
    sh:path dcat:spatialResolutionInMeters .

core-shape:dcat-Dataset-dcat-temporalResolution a sh:PropertyShape ;
    rdfs:isDefinedBy core-shape:core-shape ;
    sh:datatype xsd:duration ;
    sh:description "The minimum time period resolvable in the dataset." ;
    sh:maxCount 1 ;
    sh:name "temporal Resolution" ;
    sh:path dcat:temporalResolution .

core-shape:dcat-Dataset-dcat-theme a sh:PropertyShape ;
    sh:class skos:Concept ;
    sh:description "A category of the Dataset. Usage: A Dataset may be associated with multiple themes. Note: the theme is an URI not URL therefore, it may not properly resolve in the web browser even though it properly resolves through the API." ;
    sh:name "theme" ;
    sh:path dcat:theme ;
    sh:sparql [ sh:select "SELECT ?this ?that WHERE { ?this <http://www.w3.org/ns/dcat#theme> ?that . ?that <http://www.w3.org/ns/dcat#theme> ?this .}" ] .

core-shape:dcat-Dataset-dcatap-applicablelegislation a sh:PropertyShape ;
    sh:class eli:LegalResource ;
    sh:description "The legislation that mandates the creation or management of the Dataset. Usage: DCAT-AP-LU provides additional usage guidance for this property in the DGA context." ;
    sh:name "applicablelegislation" ;
    sh:path dcatap:applicablelegislation ;
    sh:sparql [ sh:select "SELECT ?this ?that WHERE { ?this <http://data.europa.eu/r5r/applicablelegislation> ?that . ?that <http://data.europa.eu/r5r/applicablelegislation> ?this .}" ] .

core-shape:dcat-Dataset-dct-accessRights a sh:PropertyShape ;
    sh:class dcterms:RightsStatement ;
    sh:description "Information that indicates whether the Dataset is publicly accessible, has access restrictions or is not public. Usage note: DCAT-AP-LU places additional restrictions on the vocabularies recommended in DCAT-AP. See section 10. Controlled Vocabularies for details." ;
    sh:maxCount 1 ;
    sh:name "access Rights" ;
    sh:path dcterms:accessRights ;
    sh:sparql [ sh:select "SELECT ?this ?that WHERE { ?this <http://purl.org/dc/terms/accessRights> ?that . ?that <http://purl.org/dc/terms/accessRights> ?this .}" ] .

core-shape:dcat-Dataset-dct-accrualPeriodicity a sh:PropertyShape ;
    sh:class dcterms:Frequency ;
    sh:description "The frequency at which the Dataset is updated." ;
    sh:maxCount 1 ;
    sh:name "accrual Periodicity" ;
    sh:path dcterms:accrualPeriodicity ;
    sh:sparql [ sh:select "SELECT ?this ?that WHERE { ?this <http://purl.org/dc/terms/accrualPeriodicity> ?that . ?that <http://purl.org/dc/terms/accrualPeriodicity> ?this .}" ] .

core-shape:dcat-Dataset-dct-conformsTo a sh:PropertyShape ;
    sh:class dcterms:Standard ;
    sh:description "An implementing rule or other specification." ;
    sh:name "conforms To" ;
    sh:path dcterms:conformsTo ;
    sh:sparql [ sh:select "SELECT ?this ?that WHERE { ?this <http://purl.org/dc/terms/conformsTo> ?that . ?that <http://purl.org/dc/terms/conformsTo> ?this .}" ] .

core-shape:dcat-Dataset-dct-creator a sh:PropertyShape ;
    sh:class foaf:Agent ;
    sh:description "An entity responsible for producing the dataset." ;
    sh:maxCount 1 ;
    sh:name "creator" ;
    sh:path dcterms:creator ;

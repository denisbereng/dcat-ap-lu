@prefix ex:   <http://example.org/> .


#  Circular reference and wrong Class
ex:dataset-invalid3 a dcat:Dataset ;
    dct:identifier "dataset-invalid-3" ;
    dct:title "Circular temporal reference" ;
    dct:description "Temporal resource references back to the dataset." ;
    dcat:distribution ex:dist-c ;
    dct:temporal ex:dataset-invalid3 .


ex:dist-c a dcat:Distribution ;
           dcat:accessURL <http://example.org/download/dataset-003> .


@prefix dcat: <http://www.w3.org/ns/dcat#> .
@prefix ex: <http://example.org/> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .

#  Wrong class (not a dcat:CatalogRecord)
ex:catalog2 a dcat:Catalog ;
    dcat:record ex:record2 .

ex:record2 a rdfs:Class .

#  Circular reference
ex:catalog3 a dcat:Catalog ;
    dcat:record ex:record3 .

ex:record3 a dcat:CatalogRecord ;
    dcat:record ex:catalog3 ;
 foaf:primaryTopic ex:dataset1 .

ex:dataset1 a dcat:Resource .
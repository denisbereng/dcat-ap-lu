@prefix dcat: <http://www.w3.org/ns/dcat#> .
@prefix odrl: <http://www.w3.org/ns/odrl/2/> .
@prefix dcterms: <http://purl.org/dc/terms/> .
@prefix ex: <http://example.org/> .

ex:distribution1 a dcat:Distribution ;
    dcat:accessURL <http://example.org/download/dataset-001> ;
    dcterms:title "Valid distribution with policy"@en ;
    odrl:hasPolicy ex:policy1 .

ex:policy1 a odrl:Policy ;
    dcterms:title "Policy about usage rights"@en .

@prefix dcat:   <http://www.w3.org/ns/dcat#> .
@prefix dcterms: <http://purl.org/dc/terms/> .
@prefix ex:     <http://example.org/> .

ex:distribution1 a dcat:Distribution ;
    dcat:accessURL <http://example.org/download/dataset-001> ;
        dcterms:rights [
        a dcterms:RightsStatement ;
        dcterms:title "Public Domain"@en
    ] .

#  INVALID 1: Wrong class (uses foaf:Document)
ex:distribution1 a dcat:Distribution ;
    dcat:accessURL <http://example.org/download/dataset-001> ;
        dcterms:rights [
        a <http://xmlns.com/foaf/0.1/Document> ;
        dcterms:title "Incorrect Type"@en
    ] .

# INVALID 2: Too many values
ex:distribution2 a dcat:Distribution ;
      dcat:accessURL <http://example.org/download/dataset-002> ;
       dcterms:rights
        [ a dcterms:RightsStatement ; dcterms:title "Rights A"@en ],
        [ a dcterms:RightsStatement ; dcterms:title "Rights B"@en ] .


# INVALID 3: Circular reference
ex:distribution2 a dcat:Distribution ;
    dcat:accessURL <http://example.org/download/dataset-002> ;
    dcterms:rights ex:distribution3 .

ex:distribution3 a dcterms:RightsStatement ;
    dcterms:title "Circular Rights"@en .

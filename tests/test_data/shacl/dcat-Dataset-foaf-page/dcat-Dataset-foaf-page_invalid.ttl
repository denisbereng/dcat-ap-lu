@prefix dcat: <http://www.w3.org/ns/dcat#> .
@prefix dct:  <http://purl.org/dc/terms/> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix ex:   <http://example.org/> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .

# Dataset with invalid foaf:page values
ex:dataset-invalid a dcat:Dataset ;
    dct:identifier "dataset-002" ;
    dct:title "Invalid Dataset" ;
    dct:description "This dataset contains all invalid page cases" ;
    dcat:distribution ex:dist-2 ;
    #  Circular reference
    foaf:page ex:dataset-invalid .

   ex:invalidPage1 a skos:Concept ;
    skos:prefLabel "Not a Document"@en .

ex:dist-2 a dcat:Distribution ;
    dcat:accessURL <http://example.org/download/dataset-001> .

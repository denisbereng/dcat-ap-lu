@prefix dcat: <http://www.w3.org/ns/dcat#> .
@prefix dct:  <http://purl.org/dc/terms/> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix ex:   <http://example.org/> .

ex:dataset-001 a dcat:Dataset ;
    dct:identifier "dataset-001" ;
    dct:title "Valid Dataset" ;
    dct:description "A valid dataset with a correct foaf:page" ;
    dcat:distribution ex:dist-1 ;
    foaf:page ex:page-1 .

ex:page-1 a foaf:Document ;
    foaf:homepage <http://example.org/dataset-info> .

ex:dist-1 a dcat:Distribution ;
       dcat:accessURL <http://example.org/download/dataset-001> .


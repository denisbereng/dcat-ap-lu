@prefix dcat:   <http://www.w3.org/ns/dcat#> .
@prefix dct:    <http://purl.org/dc/terms/> .
@prefix dcatap: <http://data.europa.eu/r5r/> .
@prefix eli:    <http://data.europa.eu/eli/ontology#> .
@prefix ex:     <http://example.org/> .

ex:dataset-invalid a dcat:Dataset ;
    dct:identifier "dataset-002" ;
    dct:title "Dataset with Invalid Applicable Legislation"@en ;
    dct:description "Demonstrates violations of SHACL shape for applicable legislation."@en ;
    dcat:distribution ex:dist1 ;

    #  Invalid 1: not a eli:LegalResource
    dcatap:applicablelegislation ex:NotALegalResource ;

    #  Invalid 2: circular reference (dataset links to law, and law links back to dataset)
    dcatap:applicablelegislation ex:circularLaw .

ex:NotALegalResource a dcat:test ;
    dct:title "Wrong type"@en .

ex:circularLaw a eli:LegalResource ;
    dcatap:applicablelegislation ex:dataset-invalid ;
    dct:title "Circular Law"@en .

ex:dist1 a dcat:Distribution ;
    dcat:accessURL <http://example.org/download/dataset-001> .
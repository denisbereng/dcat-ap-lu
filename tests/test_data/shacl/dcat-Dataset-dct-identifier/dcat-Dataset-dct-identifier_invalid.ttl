@prefix dcat: <http://www.w3.org/ns/dcat#> .
@prefix dct:  <http://purl.org/dc/terms/> .
@prefix ex:   <http://example.org/> .

# Missing dct:identifier
ex:dataset-invalid a dcat:Dataset ;
    dct:title "Dataset Without Identifier"@en ;
    dct:description "This dataset is missing a required identifier."@en ;
    dcat:distribution ex:distribution1 .

ex:distribution1 a dcat:Distribution ;
    dct:title "CSV Export"@en ;
    dcat:accessURL <http://example.org/download/dataset-001> .

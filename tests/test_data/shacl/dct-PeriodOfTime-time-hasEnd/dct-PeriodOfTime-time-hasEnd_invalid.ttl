@prefix dcat: <http://www.w3.org/ns/dcat#> .
@prefix ex: <http://example.org/> .
@prefix time: <http://www.w3.org/2006/time#> .
@prefix dct: <http://purl.org/dc/terms/> .

ex:periodInvalid a dct:PeriodOfTime ;
    #  Wrong datatype: string without dateTime
    time:hasEnd "2022-01-01" ;

    #  Second value violates sh:maxCount 1
    time:hasEnd "2023-01-01T00:00:00"^^<http://www.w3.org/2001/XMLSchema#dateTime> .


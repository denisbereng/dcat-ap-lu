@prefix dcat: <http://www.w3.org/ns/dcat#> .
@prefix dct: <http://purl.org/dc/terms/> .
@prefix dcatap: <http://data.europa.eu/r5r/> .
@prefix eli: <http://data.europa.eu/eli/ontology#> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix ex: <http://example.org/> .

#  Wrong class (foaf:Document instead of eli:LegalResource)
ex:datasetSeries-wrong-class a dcat:DatasetSeries ;
    dct:identifier "ds-series-002" ;
    dct:title "Invalid Dataset Series - Wrong Class" ;
     dct:description "A datasetSeries with a datasetSeries applicablelegislation info." ;
    dcatap:applicablelegislation ex:wrong-class-datasetSeries .

ex:wrong-class-legislation a foaf:Document ;
    dct:title "Not a LegalResource"@en .

#  Circular reference
ex:datasetSeries-circular a dcat:DatasetSeries ;
    dct:identifier "ds-series-003" ;
    dct:title "Invalid Dataset Series - Circular Reference" ;
     dct:description "A datasetSeries with a circular datasetSeries info." ;
    dcatap:applicablelegislation ex:legislation-1 .

ex:legislation-1 a eli:LegalResource ;
   dcatap:applicablelegislation ex:datasetSeries-circular.


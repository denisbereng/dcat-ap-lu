@prefix dcat: <http://www.w3.org/ns/dcat#> .
@prefix dct: <http://purl.org/dc/terms/> .
@prefix ex: <http://example.org/> .

ex:dataset-valid a dcat:Dataset ;
    dct:identifier "dataset-003" ;
    dct:title "Valid Dataset"@en ;
    dct:description "Dataset with valid accrual periodicity."@en ;
    dcat:distribution ex:dist1 ;
    dct:accrualPeriodicity [
        a dct:Frequency ;
        dct:title "Annual"@en
    ] .
ex:dist1 a dcat:Distribution ;
    dcat:accessURL <http://example.org/download/dataset-001> .
@prefix dcat: <http://www.w3.org/ns/dcat#> .
@prefix dct: <http://purl.org/dc/terms/> .
@prefix ex: <http://example.org/> .

# Violates multiple constraints
ex:locationInvalid a dct:Location ;

    #  Not a literal — using an IRI instead
    dcat:centroid ex:notALiteral ;

    #  Too many values — violates sh:maxCount 1
    dcat:centroid "POINT(6.13 49.61)" ;
    dcat:centroid "POINT(6.14 49.62)" .

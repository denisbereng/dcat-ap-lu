@prefix dcat: <http://www.w3.org/ns/dcat#> .
@prefix dct:  <http://purl.org/dc/terms/> .
@prefix ex:   <http://example.org/> .

ex:dataset-v1 a dcat:Dataset ;
    dct:identifier "dataset-v1" ;
    dct:title "Version 1 of the dataset"@en ;
    dct:description "First version of the dataset."@en ;
    dcat:distribution ex:dist1 .

ex:dataset-v2 a dcat:Dataset ;
    dct:identifier "dataset-v2" ;
    dct:title "Version 2 of the dataset"@en ;
    dct:description "Second version, building on version 1."@en ;
    dct:isVersionOf ex:dataset-v1 ;
    dcat:distribution ex:dist2 .

ex:dist1 a dcat:Distribution ;
    dct:title "CSV Format v1"@en ;
     dcat:accessURL <http://example.org/download/dataset-001> .

ex:dist2 a dcat:Distribution ;
    dct:title "CSV Format v2"@en ;
    dcat:accessURL <http://example.org/download/dataset-002> .

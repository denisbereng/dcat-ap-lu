@prefix dcat: <http://www.w3.org/ns/dcat#> .
@prefix dct:  <http://purl.org/dc/terms/> .
@prefix ex:   <http://example.org/> .

ex:dataset-002 a dcat:Dataset ;
    dct:identifier "dataset-002" ;
    dct:title "Invalid Dataset" ;
    dct:description "Invalid modified values." ;
    dcat:distribution ex:dist2 ;

    # Violation 1: Wrong datatype (plain literal)
    dct:modified "2024-12-31" ;

    # Violation 2: Second value, violating maxCount=1
    dct:modified "2025-01-01T12:00:00"^^<http://www.w3.org/2001/XMLSchema#dateTime> .

ex:dist2 a dcat:Distribution ;
    dcat:accessURL <http://example.org/download/dataset-001> .

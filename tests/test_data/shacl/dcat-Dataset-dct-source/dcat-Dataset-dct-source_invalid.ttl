@prefix dcat: <http://www.w3.org/ns/dcat#> .
@prefix dct:  <http://purl.org/dc/terms/> .
@prefix ex:   <http://example.org/> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .


# Case 1: Wrong class (not dcat:Dataset)
ex:dataset-invalid1 a dcat:Dataset ;
    dct:identifier "dataset-invalid-001" ;
    dct:title "Dataset with wrong class as source" ;
    dct:description "Invalid due to source not being dcat:Dataset." ;
    dcat:distribution ex:dist-1 ;
    dct:source ex:not-a-dataset .

ex:not-a-dataset a foaf:Document .


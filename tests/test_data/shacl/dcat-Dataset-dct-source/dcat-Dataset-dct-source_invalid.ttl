#  Case 3: Circular reference
ex:dataset-invalid3 a dcat:Dataset ;
    dct:identifier "dataset-invalid-003" ;
    dct:title "Dataset with circular source" ;
    dct:description "Invalid due to circular reference." ;
    dcat:distribution ex:dist-3 ;
    dct:source ex:dataset-invalid3 .

ex:dist-1 a dcat:Distribution ;
    dcat:accessURL <http://example.org/download/dataset-002> .

ex:dist-2 a dcat:Distribution ;
   dcat:accessURL <http://example.org/download/dataset-002> .
ex:dist-3 a dcat:Distribution ;
    dcat:accessURL <http://example.org/download/dataset-003> .


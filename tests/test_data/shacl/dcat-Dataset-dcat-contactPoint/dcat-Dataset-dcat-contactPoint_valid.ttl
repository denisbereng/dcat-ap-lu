@prefix dcat:  <http://www.w3.org/ns/dcat#> .
@prefix dct:   <http://purl.org/dc/terms/> .
@prefix vcard: <http://www.w3.org/2006/vcard/ns#> .
@prefix ex:    <http://example.org/> .

ex:dataset-001 a dcat:Dataset ;
    dct:identifier "dataset-001" ;
    dct:title "Valid Dataset" ;
    dct:description "Dataset with a valid contact point." ;
    dcat:distribution ex:dist1 ;
   dcat:contactPoint [
    a vcard:Kind, vcard:Organization ;
    vcard:fn "Luxembourg National Data Service"@eng
  ] .

ex:dist1 a dcat:Distribution ;
    dcat:accessURL <http://example.org/download/dataset-001> .
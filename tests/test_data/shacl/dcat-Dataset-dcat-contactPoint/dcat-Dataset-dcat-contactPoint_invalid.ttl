### 1. Wrong class (foaf:Agent)
ex:dataset-foaf a dcat:Dataset ;
    dcterms:identifier "dataset-foaf" ;
    dcterms:title "Dataset with foaf:Agent" ;
    dcterms:description "Invalid: contactPoint is foaf:Agent instead of vcard:Kind" ;
    dcat:distribution ex:dist-1 ;
    dcat:contactPoint [
        a foaf:Agent ;
        foaf:name "Helpdesk"
    ] .


### 2. Circular reference (loop)
ex:dataset-loop a dcat:Dataset ;
    dcterms:identifier "dataset-loop" ;
    dcterms:title "Dataset with Circular Contact Point" ;
    dcterms:description "Invalid: contactPoint is circular reference" ;
    dcat:distribution ex:dist-1 ;
    dcat:contactPoint ex:loop-contact .

ex:loop-contact a vcard:Kind ;
    dcat:contactPoint ex:dataset-loop ;
    vcard:fn "Circular Team" .

### Dummy distribution
ex:dist-1 a dcat:Distribution ;
       dcat:accessURL <http://example.org/download/dataset-001> .

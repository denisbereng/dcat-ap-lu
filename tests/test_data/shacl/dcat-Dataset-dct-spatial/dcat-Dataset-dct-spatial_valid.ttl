@prefix dcat: <http://www.w3.org/ns/dcat#> .
@prefix dct:  <http://purl.org/dc/terms/> .
@prefix ex:   <http://example.org/> .

ex:dataset1 a dcat:Dataset ;
    dct:identifier "dataset-001" ;
    dct:title "Dataset with valid spatial coverage" ;
    dct:description "Covers Luxembourg." ;
    dcat:distribution ex:dist-1 ;
    dct:spatial ex:luxembourg .

ex:luxembourg a dct:Location ;
    dct:title "Luxembourg" .

ex:dist-1 a dcat:Distribution ;
       dcat:accessURL <http://example.org/download/dataset-001> .

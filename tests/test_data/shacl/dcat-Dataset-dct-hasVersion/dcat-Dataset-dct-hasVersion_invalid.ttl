@prefix dcat: <http://www.w3.org/ns/dcat#> .
@prefix dct:  <http://purl.org/dc/terms/> .
@prefix ex:   <http://example.org/> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .


ex:dataset-invalid a dcat:Dataset ;
    dct:identifier "dataset-invalid" ;
    dct:title "Dataset Invalid"@en ;
    dct:description "An invalid example missing proper version references."@en ;
    dcat:distribution ex:dist-v1 ;

    # Invalid 1: target is NOT a dcat:Dataset
    dct:hasVersion ex:not-a-dataset ;

    # Invalid 2: Circular reference (see SPARQL anti-pattern)
    dct:hasVersion ex:dataset-invalid .

ex:not-a-dataset a foaf:Document ;
    dct:title "This is not a Dataset"@en .
ex:dist-v1 a dcat:Distribution ;
    dcat:accessURL <http://example.org/download/dataset-001> .
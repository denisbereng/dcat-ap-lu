@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix dcterms: <http://purl.org/dc/terms/> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix ex: <http://example.org/> .

# Violations:
# - Two values for dct:type → violates sh:maxCount 1
# - One of the values (ex:type2) is not a skos:Concept → violates sh:class
# - Circular reference: ex:agent2 <-> ex:type3

ex:agent2 a foaf:Agent ;
    foaf:name "PNED GIE"@eng;

    dcterms:type ex:type1, ex:type2, ex:type3 .

ex:type1 a skos:Concept ;
    skos:prefLabel "Team"@en .

ex:type2 a foaf:Document ;  # Wrong class
    dcterms:title "Wrong class" .

ex:type3 a skos:Concept ;
    skos:prefLabel "Team 3"@en ;
    dcterms:type ex:agent2 .  # Circular reference

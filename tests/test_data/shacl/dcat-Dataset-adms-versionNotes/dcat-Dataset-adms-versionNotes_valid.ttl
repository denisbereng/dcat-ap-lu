@prefix dcat: <http://www.w3.org/ns/dcat#> .
@prefix dct:  <http://purl.org/dc/terms/> .
@prefix adms: <http://www.w3.org/ns/adms#> .
@prefix ex:   <http://example.org/> .

ex:dataset-valid a dcat:Dataset ;
    dct:identifier "dataset-valid" ;
    dct:title "Valid Dataset" ;
    dct:description "A dataset with correct versionNotes." ;
    dcat:distribution ex:dist1 ;
    adms:versionNotes "This version includes updated metadata fields." .

ex:dist1 a dcat:Distribution ;
    dcat:accessURL <http://example.org/download/dataset-001> .

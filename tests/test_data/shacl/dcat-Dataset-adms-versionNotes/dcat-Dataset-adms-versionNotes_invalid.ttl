@prefix dcat: <http://www.w3.org/ns/dcat#> .
@prefix dct:  <http://purl.org/dc/terms/> .
@prefix adms: <http://www.w3.org/ns/adms#> .
@prefix ex:   <http://example.org/> .

ex:dataset-001 a dcat:Dataset ;
    dct:identifier "dataset-001" ;
    dct:title "Invalid Dataset" ;
    dct:description "This dataset has incorrect versionNotes usage." ;
    dcat:distribution ex:dist1 ;

  #  versionNotes is an IRI, not a literal
    adms:versionNotes ex:NoteResource .

# An IRI used instead of a string literal
ex:NoteResource a dct:ProvenanceStatement ;
    dct:description "This should not be used as adms:versionNotes." .

ex:dist1 a dcat:Distribution ;
    dcat:accessURL <http://example.org/download/dataset-001> .
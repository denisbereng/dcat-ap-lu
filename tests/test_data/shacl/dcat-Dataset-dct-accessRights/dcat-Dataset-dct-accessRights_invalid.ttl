@prefix dcat:   <http://www.w3.org/ns/dcat#> .
@prefix dct:    <http://purl.org/dc/terms/> .
@prefix ex:     <http://example.org/> .
@prefix skos:   <http://www.w3.org/2004/02/skos/core#> .

ex:dataset-invalid a dcat:Dataset ;
    dct:identifier "dataset-002" ;
    dct:title "Invalid Dataset"@en ;
    dct:description "A dataset with invalid access rights."@en ;
    dcat:distribution ex:dist1 ;

    #  Invalid 1: More than one accessRights (maxCount violation)
    dct:accessRights ex:rights1, ex:rights2 .

ex:rights1 a dct:RightsStatement ;
    skos:prefLabel "Public"@en .

ex:rights2 a dct:RightsStatement ;
    skos:prefLabel "Non-Public"@en .

#  Invalid 2: Circular reference
ex:dataset-invalid dct:accessRights ex:loop .
ex:loop a dct:RightsStatement ;
    dct:accessRights ex:dataset-invalid .
ex:dist1 a dcat:Distribution ;
    dcat:accessURL <http://example.org/download/dataset-001> .
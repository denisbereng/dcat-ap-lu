@prefix dcat:   <http://www.w3.org/ns/dcat#> .
@prefix dct:    <http://purl.org/dc/terms/> .
@prefix skos:   <http://www.w3.org/2004/02/skos/core#> .
@prefix dcatap: <http://data.europa.eu/r5r/> .
@prefix adms:   <http://www.w3.org/ns/adms#> .
@prefix foaf:   <http://xmlns.com/foaf/0.1/> .
@prefix owl:    <http://www.w3.org/2002/07/owl#> .
@prefix vcard:  <http://www.w3.org/2006/vcard/ns#> .
@prefix spdx:   <http://spdx.org/rdf/terms#> .
@prefix xsd:    <http://www.w3.org/2001/XMLSchema#> .
@prefix :       <https://example.org/dcat-ap-lu-examples/> .
@prefix rdfs:   <http://www.w3.org/2000/01/rdf-schema#> .
@prefix csvw: <http://www.w3.org/ns/csvw#> .


#################################################################
# Dataset Series (mandatory dct:title and dct:description)
#################################################################

:series-001
    a dcat:DatasetSeries ;
    dct:title "Example Dataset Series"@en ;
    dct:description "This series groups related datasets including different versions."@en .

#################################################################
# Dataset with required properties and link to series
#################################################################

:dataset-full
    a dcat:Dataset ;
    dct:identifier "dataset-full" ;
    dct:title "Full Dataset Example"@en ;
    dct:description "A fully populated dataset using controlled vocabularies with rdfs:seeAlso."@en ;
    adms:identifier :identifier-001 ;
    dcat:inSeries :series-001 ;
    dct:accrualPeriodicity [ a dct:Frequency ;
        rdfs:seeAlso <http://publications.europa.eu/resource/authority/frequency/WEEKLY> ;
        skos:prefLabel "Weekly"@en ] ;
    dct:accessRights [ a dct:RightsStatement ;
        rdfs:seeAlso <https://publications.europa.eu/resource/authority/access-right/PUBLIC> ;
        skos:prefLabel "Public"@en ] ;
    dct:type [ a skos:Concept ;
        rdfs:seeAlso <https://publications.europa.eu/resource/authority/dataset-type/ARCHIVED_DATA> ;
        skos:prefLabel "Archived data"@en ] ;
    dcat:theme
        [ a skos:Concept ;
          rdfs:seeAlso <https://publications.europa.eu/resource/authority/data-theme/GOVE> ;
          skos:prefLabel "Government"@en ],
        [ a skos:Concept ;
          rdfs:seeAlso <https://publications.europa.eu/resource/authority/data-theme/TECH> ;
          skos:prefLabel "Technology"@en ] ;
    dct:language [ a dct:LinguisticSystem ;
        rdfs:seeAlso <https://publications.europa.eu/resource/authority/language/ENG> ;
        skos:prefLabel "English"@en ] ;
    dct:issued "2025-08-01T12:00:00"^^xsd:dateTime ;
    dct:modified "2025-08-06T12:00:00"^^xsd:dateTime ;
    dct:publisher [ a foaf:Agent ; foaf:name "Publishing Org"@en ] ;
    dct:creator [ a foaf:Agent ; foaf:name "Creating Org"@en ] ;
    dcat:distribution :dist-full-csv, :dist-full-json ;
    dcatap:availability [
        a skos:Concept ;
        rdfs:seeAlso <http://publications.europa.eu/resource/authority/planned-availability/TEMPORARY> ;
        skos:prefLabel "Temporary"@en
    ] ;
    adms:versionNotes "Version 1"@en ;
    dcat:contactPoint [
        a vcard:Kind ;
        vcard:fn "Help Desk"@en ;
        vcard:hasEmail <mailto:help@example.org>
    ] ;
    dct:isVersionOf :dataset-002 ;
    dct:hasVersion :dataset-full-v2 ;
    dct:source :dataset-001 ;
    adms:identifier :identifier-001 ;
    dct:isReferencedBy <https://example.org/pub/1> ;
    dct:temporal [
        a dct:PeriodOfTime ;
        dcat:startDate "2024-08-06T12:00:00"^^xsd:dateTime ;
        dcat:endDate "2025-07-06T12:00:00"^^xsd:dateTime ;
    ] ;
        dcat:keyword"CSV format"@en ;
    foaf:page [
    a foaf:Document ;
    csvw:column [
      csvw:name "variable_1" ;
      csvw:datatype xsd:boolean ;
      dct:description "This is a test description for field 1"@eng
    ]];
    dcat:spatialResolutionInMeters "2.2"^^xsd:decimal  ;
    foaf:page [
    a foaf:Document ;
    rdfs:seeAlso <https://example.full.data>
  ] ;
    dct:spatial [
    a dct:Location ;
    rdfs:seeAlso <https://publications.europa.eu/resource/authority/country/LUX> ;
    skos:prefLabel "Luxembourg"@eng
  ], [
    a dct:Location ;
    rdfs:seeAlso <https://publications.europa.eu/resource/authority/country/LTU> ;
    skos:prefLabel "Lithuania"@eng
  ] .

:identifier-001
    a adms:Identifier ;
    skos:notation "ID-001" .


:relationship-001
    a dcat:Relationship ;
    dcat:qualifiedRelation :dataset-full .

#################################################################
# Other Dataset resources
#################################################################

:dataset-001
    a dcat:Dataset ;
    dct:identifier "dataset-001" ;
    dct:title "Previous Version Dataset"@en ;
    dct:description "Used as the source dataset."@en .

:dataset-002
    a dcat:Dataset ;
    dct:identifier "dataset-002" ;
    dct:title "Parent Dataset"@en ;
    dct:description "A parent version of dataset-full."@en .

:dataset-full-v2
    a dcat:Dataset ;
    dct:identifier "dataset-full-v2" ;
    dct:title "Next Version of Full Dataset"@en ;
    dct:description "A newer version of dataset-full."@en .

#################################################################
# Distributions
#################################################################

:dist-full-csv
    a dcat:Distribution ;
    dct:title "Full CSV Export"@en ;
    dct:description "CSV format of dataset-full."@en ;
    dct:identifier "dist-full-csv" ;
    dct:language [ a dct:LinguisticSystem ;
        rdfs:seeAlso <https://publications.europa.eu/resource/authority/language/ENG> ;
        skos:prefLabel "English"@en ] ;
    dct:license [ a dct:LicenseDocument ;
        rdfs:seeAlso <http://publications.europa.eu/resource/authority/licence/CC0> ;
        skos:prefLabel "CC0"@en ] ;
           dct:rights [ a dct:RightsStatement ;
        rdfs:seeAlso <https://publications.europa.eu/resource/authority/access-right/PUBLIC> ;
        skos:prefLabel "Public"@en ] ;
        dcat:accessURL [
    a rdfs:Resource ;
    rdfs:seeAlso <https://example.org/data/full.csv>
  ] ;
      dct:format [
    a dct:MediaTypeOrExtent ;
    rdfs:seeAlso <http://publications.europa.eu/resource/authority/file-type/csv>
  ] ;
     dcat:downloadURL [
    a rdfs:Resource ;
    rdfs:seeAlso <https://example.org/data/full.csv/download> ;
  ] ;
    dcat:mediaType [ a dct:MediaType ;
        rdfs:seeAlso <https://www.iana.org/assignments/media-types/text/csv> ;
        skos:prefLabel "text/csv"@en ] ;
    dcat:compressFormat [ a dct:MediaType ;
        rdfs:seeAlso <https://www.iana.org/assignments/media-types/application/gzip> ;
        skos:prefLabel "application/gzip"@en ] ;
    dcat:packageFormat [ a dct:MediaType ;
        rdfs:seeAlso <https://www.iana.org/assignments/media-types/application/zip> ;
        skos:prefLabel "application/zip"@en ] ;
    adms:status [ a skos:Concept ;
        rdfs:seeAlso <http://publications.europa.eu/resource/authority/distribution-status/COMPLETED> ;
        skos:prefLabel "Completed"@en ] ;
    dcatap:availability [
        a skos:Concept ;
        rdfs:seeAlso <http://publications.europa.eu/resource/authority/planned-availability/TEMPORARY> ;
        skos:prefLabel "Temporary"@en
    ] ;
    dcat:byteSize "123456"^^xsd:nonNegativeInteger ;
    spdx:checksum [ a spdx:Checksum ;
        spdx:algorithm "SHA-256" ;
        spdx:checksumValue "abcdef012" ] .

:dist-full-json
    a dcat:Distribution ;
    dct:title "Full JSON Export"@en ;
    dct:description "JSON format of dataset-full."@en ;
    dct:identifier "dist-full-json" ;
    dcat:accessURL [
    a rdfs:Resource ;
    rdfs:seeAlso  <https://example.org/data/full.json>
  ] ;
    dcat:downloadURL [
    a rdfs:Resource ;
    rdfs:seeAlso <https://example.org/data/full.json/download> ;
  ] ;
    dcat:mediaType [ a dct:MediaType ;
        rdfs:seeAlso <https://www.iana.org/assignments/media-types/application/json> ;
        skos:prefLabel "application/json"@en ] ;
    dct:format [
    a dct:MediaTypeOrExtent ;
    rdfs:seeAlso <http://publications.europa.eu/resource/authority/file-type/json>
  ] ;
    dct:license [ a dct:LicenseDocument ;
        rdfs:seeAlso <http://publications.europa.eu/resource/authority/licence/CC_BYNCND_2_5> ;
        skos:prefLabel "CC BY-NC-ND 2.5"@en ] ;
    adms:status [ a skos:Concept ;
        rdfs:seeAlso <http://publications.europa.eu/resource/authority/distribution-status/DEVELOP> ;
        skos:prefLabel "Develop"@en ] .

@prefix dcat: <http://www.w3.org/ns/dcat#> .
@prefix dcterms: <http://purl.org/dc/terms/> .
@prefix ex: <http://example.org/> .

# Distribution with compressFormat but:
# - compressFormat points to a resource that is not a dcterms:MediaType (violation of sh:class)
ex:distribution1 a dcat:Distribution ;
    dcat:accessURL <http://example.org/download/dataset-001> ;
    dcat:compressFormat ex:gzipFormat .

# gzipFormat has wrong class (should be dcterms:MediaType)
ex:gzipFormat a dcterms:MediaType ;
    dcterms:title "GZIP" .



